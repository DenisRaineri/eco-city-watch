{
  "expo": {
    "name": "EcoCity Watch",
    "slug": "eco-city-watch",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ecocitywatch",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "Monitor Ambiental Urbano - Aplicativo desenvolvido para monitoramento de condições ambientais urbanas",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#10b981"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-location",
      "expo-camera"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
